<%= render "list", user: current_user %>
<%= render "books/list", book: @book %>



<h1>Users</h1>

<% @users.each do |user| %>

<div>
  <%#= profile_image_tag user.get_profile_image %>
  <%#= profile_image_tag 'no_image.jpg' %>
  <p>Name</p>
  <%= user.name %>
</div>
<%= link_to "show", user_path(user.id) %>
<% end %>


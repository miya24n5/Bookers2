
  <table class="table table-striped">
<h1>New book</h1>
  <%= form_with model: book, local:true do |f| %>

    <h4>Title</h4>
      <%= f.text_field :title %>

    <h4>Opinion</h4>
      <%= f.text_area :body %>

    <%= f.submit 'Create Book', class: "btn btn-success btn-sm mt-3 mb-3" %>

  <% end %>

  </table>